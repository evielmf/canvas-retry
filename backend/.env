# FastAPI Backend Environment Configuration
# Copy to .env and configure with your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/canvas_retry
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_KEY=your_supabase_service_key

# Security
SECRET_KEY=your_super_secret_key_here_at_least_32_characters_long
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Canvas API Configuration
CANVAS_ENCRYPTION_KEY=your_canvas_encryption_key_32_chars
MAX_CONCURRENT_REQUESTS=10
REQUEST_TIMEOUT=30
RETRY_ATTEMPTS=3

# Redis Configuration (for background tasks)
REDIS_URL=redis://localhost:6379

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json

# Rate Limiting
RATE_LIMIT_PER_MINUTE=60

# Environment
ENVIRONMENT=development
